   "source": [
    "num = int(input())\n",
    "rev = 0\n",
    "sum_even = 0\n",
    "sum_odd = 0\n",
    "while num >0:\n",
    "    r = num % 10\n",
    "    rev = rev * 10 + r\n",
    "    num = num // 10\n",
    "    if rev % 2 == 0:\n",
    "        sum_even = sum_even + r\n",
    "    else:\n",
    "        sum_odd = sum_odd + r\n",
    "print(sum_even,\"\",sum_odd)\n"
   ]
